{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "device_lcr-telemetry-impedance-point",
  "description": "单点阻抗测量数据。",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "telemetry",
    "corr_id",
    "device_id",
    "timestamp",
    "freq_hz",
    "impedance_ohm",
    "phase_deg"
  ],
  "properties": {
    "telemetry": {
      "type": "string",
      "const": "lcr.impedance_point"
    },
    "corr_id": {
      "type": "string",
      "minLength": 1
    },
    "device_id": {
      "type": "string",
      "minLength": 1
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "freq_hz": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "impedance_ohm": {
      "type": "number",
      "minimum": 0
    },
    "phase_deg": {
      "type": "number"
    },
    "excitation_voltage_v": {
      "type": "number",
      "minimum": 0
    },
    "metadata": {
      "type": "object"
    }
  }
}
